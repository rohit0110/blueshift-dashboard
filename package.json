{
  "name": "blueshift-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NEXT_PUBLIC_COMMIT_HASH=$(git rev-parse HEAD) next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "precompile-mdx": "ts-node -r tsconfig-paths/register -P ./tsconfig.scripts.json ./scripts/precompile-mdx.ts",
    "generate-banners": "NEXT_PUBLIC_RELEASED_COURSES='*' NEXT_PUBLIC_RELEASED_CHALLENGES='*' ts-node -r tsconfig-paths/register -P ./tsconfig.scripts.json ./scripts/generate-banners.ts"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@blueshift-gg/faucet-react": "^0.2.2",
    "@blueshift-gg/ui-components": "^0.1.69",
    "@coral-xyz/anchor": "^0.31.1",
    "@monaco-editor/react": "^4.7.0",
    "@next/third-parties": "^15.3.2",
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "^9.1.2",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/wallet-adapter-phantom": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.38",
    "@solana/wallet-adapter-react-ui": "^0.9.38",
    "@solana/wallet-adapter-solflare": "^0.6.31",
    "@solana/web3.js": "^1.98.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.83.0",
    "bs58": "^6.0.0",
    "classnames": "^2.5.1",
    "dat.gui": "^0.7.9",
    "deepmerge": "^4.3.1",
    "esbuild-wasm": "^0.25.4",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "jose": "^6.1.2",
    "monaco-editor": "^0.54.0",
    "motion": "^12.6.3",
    "motion-plus": "^1.5.1",
    "next": "15.5.9",
    "next-intl": "^4.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "sonner": "^2.0.7",
    "three": "^0.181.1",
    "usehooks-ts": "^3.1.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.13.1",
    "@shikijs/core": "^3.15.0",
    "@shikijs/engine-oniguruma": "^3.15.0",
    "@shikijs/langs-precompiled": "^3.15.0",
    "@shikijs/types": "^3.15.0",
    "@tailwindcss/postcss": "^4",
    "@types/dat.gui": "^0.7.13",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.176.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "safe-mdx": "^1.3.9",
    "sharp": "^0.34.3",
    "tailwindcss": "^4",
